//c program to find net salary
#include<stdio.h>
int main()
{
   float NS,GS,HRA,DA,O,D;
   printf("Enter GS:");
   scanf("%f",&GS);
   printf("Enter HRA:");
   scanf("%f",&HRA);
   printf(HRA is %f",HRA);
   printf("Enter DA:");
   scanf("%f",&DA);
   printf(DA is %f",DA);
   printf("Enter Others:");
   scanf("%f",&O);
   printf("Others is %f",O);
   D=HRA+DA+O;
   NS=GS-D;
   printf("Netsalary is %f",NS);
   return 0;
}

