//c program to find net salary
#include<stdio.h>
int main()
{
   float NS,GS,HRA,DA,O,D,A,B,C;
   printf("Enter GS:");
   scanf("%f",&GS);
   printf("\nEnter A:");
   scanf("%f",&A);
   HRA=(A/100)*GS;
   printf("\nHRA is %f",HRA);
   printf("\nEnter B:");
   scanf("%f",&B);
   DA=(B/100)*GS;
   printf("\nDA is %f",DA);
   printf("\nEnter C:");
   scanf("%f",&C);
   O=(C/100)*GS;
   printf("\nOthers is %f",O);
   D=HRA+DA+O;
   NS=GS-D;
   printf("\nNetsalary is %f",NS);
   return 0;
}ï¿¼Enter
